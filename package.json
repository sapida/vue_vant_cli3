{
  "name": "vue_vant_cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "fastclick": "^1.0.6",
    "vant": "^2.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vue-scroller": "^2.2.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-plugin-import": "^1.12.0",
    "node-sass": "^4.9.0",
    "postcss-px-to-viewport": "^1.1.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "unitToConvert": "px",
        "viewportWidth": 375,
        "unitPrecision": 3,
